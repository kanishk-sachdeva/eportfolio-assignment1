    </head>

    <body>

        <nav class="navbar navbar-expand-lg nav-menu nav-menu-about">
            <div class="container">
                <button class="navbar-toggler ms-auto bg-dark navbar-dark" type="button" data-toggle="collapse"
                    data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse ms-auto" id="navbarNavAltMarkup">
                    <div class="navbar-nav navbar-right">
                        <li><a class="nav-link" href="/">Home&nbsp;</a></li>
                        <li><a class="nav-link" href="about">About</a></li>
                        <li><a class="nav-link" href="resume">Resume</a></li>
                        <li class="active"><a class="nav-link" href="projects">Projects</a></li>
                        <li><a class="nav-link" href="services">Services</a></li>
                        <li><a class="nav-link" href="contact">Contact</a></li>
                        <li><a class="nav-link" href="/dashboard">Dashboard</a></li>
                    </div>
                </div>
            </div>
        </nav>


        <section id="services" class="services">
            <div class="container">

                <div class="section-title mb-3">
                    <h2>Projects</h2>
                    <p>My Projects</p>
                </div>

                <div class="row" style="align-items: center; overflow: -moz-hidden-unscrollable;">


                    <% projects.forEach((data) => { %>
                        <a href="/project/<%= data.slug %>" style="text-decoration: none;">
                            <div class="card mb-3 text-white bg-dark m-0 p-0" style="max-width: 100%; max-height: auto;">
    
                                <div class="row g-0">
                                    <div class="col-md-4">
                                        <img src="<%= data.imagelink %>" width="500px" height="auto"
                                            class="img-fluid rounded-start" alt="...">
                                    </div>
                                    <div class="col-md-8">
                                        <div class="card-body">
                                            <h5 class="card-title">
                                                <%= data.title %>
                                            </h5>
                                            <p class="card-text">
                                                <%- data.intro %>
    
    
                                            </p>
                                            <p style="color: #18d26e;" class="card-text"><small>Read about Project</small></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    <% }); %>

                </div>

                <div class="row card mb-3 text-white bg-dark m-0 p-4" style="max-width: 100%; max-height: auto;">
                    Checkout my academic projects at : <a style="display: inline-block;" href="http://studentweb.cencol.ca/ksachde7/">http://studentweb.cencol.ca/ksachde7/</a>
                    <br>
                    To check all my projects, Have a look on my Resume at <a href="/resume">/resume</a>
                </div>
            </div>

            </div>

            <%- include( 'partials/footer' ); %>
                    </ul>
                </div>
            </div>
        </nav>


    <section id="projects" class="projects">
      <style>
        ul {
          list-style-type: none;
          padding: 0;
          margin: 0;
        }
        li {
          margin-bottom: 5px;
        }
        h6 > a {
          color: white;
        } 
        h6>a:hover {
          color: blue;
        }
        li > h6 {
          color: #18d26e;
        }
      </style>

      <!-- ======= About Me ======= -->
      <div class="container">

        <div class="section-title mb-3">
          <h2>About Project</h2>
          <p>
            <%= project.title %>
          </p>
          <h6 class="font-italic"><%= project.description %></h6>
        </div>

        <div class="row">
          <div class="col-lg-6" data-aos="fade-right">
            <img src="<%= project.imagelink %>" class="img-fluid" alt="">
          </div>
          <div class="col-lg-6 pt-4 pt-lg-0 content" data-aos="fade-left">
            <h3 style="color: #18d26e; font-weight: 700;
                        font-size: 26px;">Project Introduction</h3>
            <p class="font-italic">
              <%- project.intro %>
              <ul>
                <% project.data.forEach((element) => { %>
                  <%- element %>
                <% }); %>
              </ul>

              <!-- <img src="https://github-readme-stats.vercel.app/api/pin/?username=kanishk-sachdeva&repo=AzagoMovies"/> -->
              
            </p>
          </div>
        </div>

        <iframe class="bg-dark mt-5" src="<%= project.githublink %>" width="100%" height="900px"></iframe>

      </div>
      <!-- End About Me -->


      <%- include( 'partials/footer' ); %>
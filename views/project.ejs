    <nav class="navbar navbar-expand-lg nav-menu nav-menu-about">
            <div class="container-fluid">
                <button class="navbar-toggler ms-auto bg-dark navbar-dark" type="button" data-toggle="collapse"
                    data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse ms-auto" id="navbarNavAltMarkup">
                    <ul class="navbar-nav navbar-right">
                        <li><a class="nav-link" href="/">Home&nbsp;</a></li>
                        <li><a class="nav-link" href="../about">About</a></li>
                        <li><a class="nav-link" href="../resume">Resume</a></li>
